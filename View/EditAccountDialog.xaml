<Window x:Class="MoneyManagerGavr.View.EditAccountDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MoneyManagerGavr.View"
        mc:Ignorable="d"
        Title="EditAccountDialog" Height="450" Width="800">
    <StackPanel Margin="20">
        <TextBlock Text="Новое имя:" Margin="0 0 0 5"/>
        <TextBox x:Name="FirstNameBox" Margin="0 0 0 15"/>

        <TextBlock Text="Новая фамилия:" Margin="0 0 0 5"/>
        <TextBox x:Name="LastNameBox" Margin="0 0 0 20"/>

        <Button Content="Сохранить" Click="SaveButton_Click" Width="100" HorizontalAlignment="Right"/>
    </StackPanel>
</Window>
